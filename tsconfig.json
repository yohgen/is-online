{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "allowJs": true,
    "strictNullChecks": true,
    "resolveJsonModule": true,
    "exactOptionalPropertyTypes": false,
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "baseUrl": ".",
    "paths": {
      "~/*": ["./src/*"],
      "!config": ["./config.js"],
      "!integrations": ["./integrations/*"]
    }
  }
}
